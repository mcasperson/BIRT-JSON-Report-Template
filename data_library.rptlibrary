	row["TopicTitle"] = topicsWithZanataData[specTopicsAndZanataTranslationsIndex].topicTitle;
	row["ZanataLanguage"] = topicsWithZanataData[specTopicsAndZanataTranslationsIndex].locale;
	row["ZanataTranslationState"] = topicsWithZanataData[specTopicsAndZanataTranslationsIndex].translated;
	row["ZanataTranslationCount"] = topicsWithZanataData[specTopicsAndZanataTranslationsIndex].value;

	++specTopicsAndZanataTranslationsIndex;
    return true;
}

return false;]]></method>
