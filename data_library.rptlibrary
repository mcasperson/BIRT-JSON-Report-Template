            <method name="open"><![CDATA[specTopicsAndZanataTranslationsIndex = 0;

latestDocumentNamesMap = {};
latestDocumentNamesArray = [];
unpushedTopics = 0;
topicsWithZanataData = [];

/*
	Loop through all the documents available in Zanata and pull out the latest revisions
*/
Packages.java.lang.System.out.println("Finding latest Zanata Documents" );
for (docIndex = 0; docIndex < zanataCount; ++ docIndex)
{
	var docName = zanataController.getValue("items", "name", docIndex);
    
    var docNameSplit = docName.split("-");
    
    if (docNameSplit.length == 2)
    {
		if (latestDocumentNamesMap.hasOwnProperty(docNameSplit[0]))
		{
			if (parseInt(docNameSplit[1]) > latestDocumentNamesMap[docNameSplit[0]])
			{
				latestDocumentNamesMap[docNameSplit[0]] = parseInt(docNameSplit[1]);				
			}
		}
		else
		{
			latestDocumentNamesMap[docNameSplit[0]] = parseInt(docNameSplit[1]);
			latestDocumentNamesArray.push(docNameSplit[0]);
		}
    }
}

/*
	Match up the zanata translations stats with the topics
*/
Packages.java.lang.System.out.println("Matching Topics With Latest Zanata Documents" );
for (topicIndex = 0; topicIndex < specTopicsAndZanataCount; ++topicIndex)
{
	//if (topicIndex == 1)
		//break;
	
	var topicID = specTopicsAndZanataController.getValue("items", "item/id", topicIndex).toString();
    var topicTitle = specTopicsAndZanataController.getValue("items", "item/title", topicIndex);
    
    if (latestDocumentNamesMap.hasOwnProperty(topicID))
    {
    	// Create instance of the Controller class
		var zanataStatsController = new Packages.com.actuate.json.JSONParser();
		
		// The Zanata headers
		var headers = new Packages.java.util.HashMap();
		headers.put("X-Auth-User", zanataUsername);
		headers.put("X-Auth-Token", zanataApiKey);
		headers.put("Accept", "application/json");
		
		var zanataStatsURL = zanataBaseUrl + "/stats/proj/skynet-topics/iter/1/doc/" + topicID + "-" + latestDocumentNamesMap[topicID];
		
		var percentDone = (topicIndex / specTopicsAndZanataCount * 100).toFixed(2);
		
		Packages.java.lang.System.out.println(percentDone + "% Zanata Document Stats URL: " + zanataStatsURL);
		zanataStatsController.loadData(zanataStatsURL, headers, true, null);
		
		zanataStatsCount = zanataStatsController.getChildCount("stats");
		
		for (statsIndex = 0; statsIndex < zanataStatsCount; ++statsIndex)
		{
			var unit = zanataStatsController.getValue("stats", "unit", statsIndex);
			
			if (unit == "MESSAGE")
			{
				var locale = zanataStatsController.getValue("stats", "locale", statsIndex);
				var untranslated = zanataStatsController.getValue("stats", "untranslated", statsIndex);
	    		var translated = zanataStatsController.getValue("stats", "total", statsIndex) - untranslated;
	    		
	    		var translatedObject = {topicID: topicID, topicTitle: topicTitle, locale: locale, translated: "Translated", value: translated};
	    		var untranslatedObject = {topicID: topicID, topicTitle: topicTitle, locale: locale, translated: "Untranslated", value: untranslated};

	    		topicsWithZanataData.push(translatedObject);
	    		topicsWithZanataData.push(untranslatedObject);
    		}
		}
		
		// Rate limiting
		Packages.java.lang.Thread.sleep(200);
    }
    else
    {
    	++unpushedTopics;
    }
}]]></method>
            <method name="fetch"><![CDATA[if (specTopicsAndZanataTranslationsIndex < topicsWithZanataData.length)
{    	
