            <method name="open"><![CDATA[logger.info("[BIRT Report Viewer] [open] [" + specTopicsAndRevisionsUUID + "] [Data Set] [SpecTopicAndRevisions]");

/*
	--------------------- Global variables ---------------------
*/

// specs mapped to topics
specTopics = [];

// This will track your current row later on
specTopicAndRevisionsCount = 0;

// use a function to limit variable scopes
(function()
{	
	// Used for debugging messages
	var totalRows = 1;
	
	// process the results of each query against a spec
	for (var specIndex = 0; specIndex < controllerCSPIDs.length; ++specIndex)
	{		
		var controller = controllers[specIndex];
		var currentSpecId = controllerCSPIDs[specIndex];
		var controllerTopicCount = controller.getChildCount("items");
		
		// process the topics in each spec query
		for (var topicIndex = 0; topicIndex < controllerTopicCount; ++topicIndex)
		{
		    var topicId = controller.getValue("items", "item/id", topicIndex);
		    var topicTitle = controller.getValue("items", "item/title", topicIndex);
		    
		    var topic = new Packages.com.actuate.json.JSONArrayIndex(topicIndex, "items");
		    			
			var childCount = parseInt(controller.getValue("items", "item/revisions/size", topicIndex));
		    for (var i = 0; i < childCount; ++i)
		    {
			    var topicRevisions = new Packages.com.actuate.json.JSONArrayIndex(i, "item/revisions/items");
			    var revisionDate = new Date(parseInt(controller.getValue([topic, topicRevisions], "item/lastModified")));
			    
			    var revisionXML = controller.getValue([topic, topicRevisions], "item/xml");	    			 					 	
			 	var revisionNumber = controller.getValue([topic, topicRevisions], "item/revision");	
			 	var revisionMessage = controller.getValue([topic, topicRevisions], "item/logDetails/message");    
    		    
    		    /*Packages.java.lang.System.out.println("Adding row " + totalRows + 
    		    	". specId: " + currentSpecId + 					
    		    	", topicId: " + topicId + 
    		    	", topicTitle: " + topicTitle + 
    		    	", topicTagName: " + tagName + 
    		    	", topicTagID: " + tag);*/
    		    ++totalRows;
    		    
    		    // save the row
    		    var newRow = {
    		    	specId: currentSpecId, 
    		    	topicId: topicId, 
    		    	topicTitle: topicTitle, 
    		    	topicRevision: revisionNumber, 
    		    	topicRevisionXML: revisionXML, 
    		    	topicRevisionDate: revisionDate, 
    		    	topicRevisionMessage: revisionMessage 
    		   	};
			    specTopics.push(newRow);	
					    			    
		    }
		}
	}
})();]]></method>
            <method name="fetch"><![CDATA[logger.info("[BIRT Report Viewer] [fetch] [" + specTopicsAndRevisionsUUID + "] [Data Set] [SpecTopicAndRevisions]");

(function(){
	var earliestEdit = new Date(0);
	var latestEdit = Date.today();
	
	if (params["EditedInLastXDays"] != null)
	{
		var daysFilter = parseInt(params["EditedInLastXDays"]);
		
		if (!isNaN(daysFilter) &amp;&amp; daysFilter > 0)
		{						
			earliestEdit = Date.today().add({ days: -daysFilter});
		}
	}

	if (params["NotEditedInLastXDays"] != null)
	{
		var notDaysFilter = parseInt(params["NotEditedInLastXDays"]);
		
		if (!isNaN(notDaysFilter) &amp;&amp; notDaysFilter > 0)
		{						
			latestEdit = Date.today().add({ days: -notDaysFilter});
		}
	}
	
	while (specTopicAndRevisionsCount < specTopics.length) {
			if (!(specTopics[specTopicAndRevisionsCount].topicRevisionDate > earliestEdit &amp;&amp; specTopics[specTopicAndRevisionsCount].topicRevisionDate < latestEdit)) {
				++specTopicAndRevisionsCount;
			}
			else {
				break;
			}
	}
})();
		
if (specTopicAndRevisionsCount < specTopics.length)
{
	(function(){
		row["SpecID"] = specTopics[specTopicAndRevisionsCount].specId.toString();
		row["TopicID"] = specTopics[specTopicAndRevisionsCount].topicId;
		row["TopicTitle"] = specTopics[specTopicAndRevisionsCount].topicTitle;
		row["TopicRevisionDate"] = specTopics[specTopicAndRevisionsCount].topicRevisionDate.toString("dd MMM yyyy hh:mm");
		row["TopicRevision"] = specTopics[specTopicAndRevisionsCount].topicRevision;
		row["TopicRevisionXML"] = specTopics[specTopicAndRevisionsCount].topicRevisionXML;
		row["TopicRevisionMessage"] = specTopics[specTopicAndRevisionsCount].topicRevisionMessage;
		
		var myRevision = parseInt(specTopics[specTopicAndRevisionsCount].topicRevision);
		
		var nextHighestRevision = -1;
		var nextHighestRevisionIndex = -1;
		for (var i = 0; i < specTopics.length; ++i) {
			
			var yourRevision = parseInt(specTopics[i].topicRevision);
			
			if (specTopics[specTopicAndRevisionsCount].topicId == specTopics[i].topicId) {
				if (yourRevision < myRevision &amp;&amp;
					yourRevision > nextHighestRevision) {
					nextHighestRevision = yourRevision;
					nextHighestRevisionIndex = i;	
				}
			}
		}
		
		if (nextHighestRevisionIndex != -1) {
			row["TopicPreviousRevisionXML"] = specTopics[nextHighestRevisionIndex].topicRevisionXML;
			row["TopicPreviousRevision"] = specTopics[nextHighestRevisionIndex].topicRevision;
			row["TopicPreviousRevisionDate"] = specTopics[nextHighestRevisionIndex].topicRevisionDate.toString("dd MMM yyyy hh:mm");
		}
				
		++specTopicAndRevisionsCount;
	})();
	return true;
}
else
{
	return false;
}

]]></method>
