            <property name="dataSource">SpecTopics</property>
            <method name="open"><![CDATA[logger.info("[open] [" + topicsAndContentSpecsUUID + "] [Data Set] [TopicsAndContentSpecs]");



/*
	--------------------- Global variables ---------------------
*/

// This will track your current row later on
topicAndContentSpecCount = 0;

// The topic details
topics = [];

/*
	--------------------- Processing ---------------------
*/


// here we take the results of the queries against the spec ids
// and combine them into a single table

// use a function to limit variable scopes
(function()
{
	var specQueryURLPrefix = "1/topics/get/json/query;tag268=1;topicXml=%5B";
	var specQueryURLPostfix = "%5D;logic=And?expand=%7B%22branches%22%3A%5B%7B%22trunk%22%3A%7B%22name%22%3A%20%22topics%22%7D%7D%5D%7D";
	
	// find the specs that each topic is included in
	for (var index = 0; index < totalCount; ++index)
	{		
		var topicId = controller.getValue("items", "item/id", index);
		var topicTitle = controller.getValue("items", "item/title", index);
		
		// Create instance of the Controller class
		var specController = new Packages.com.actuate.json.JSONParser();
		
		// Load the JSON Source
		specController.loadData(baseUrl + specQueryURLPrefix + topicId + specQueryURLPostfix);
		
		// Calculate the total rows we will have
		totalSpecCount = specController.getChildCount("items");
		
		var specIds = "";
		for (var specIndex = 0; specIndex < totalSpecCount; ++specIndex) {
			var specId = specController.getValue("items", "item/id", specIndex);	
			if (specIds.length != 0) {
				specIds += ",";
			}
			specIds += specId;
		}
		
		topics.push({topicId: topicId, topicTitle: topicTitle, specCount: totalSpecCount, specs: specIds});
	}
})();]]></method>
            <method name="fetch"><![CDATA[logger.info("[BIRT Report Viewer] [fetch] [" + topicsAndContentSpecsUUID + "] [Data Set] [TopicsAndContentSpecs]");

if (topicAndContentSpecCount < topics.length)
{
	row["TopicID"] = topics[topicAndContentSpecCount].topicId.toString();
	row["TopicTitle"] = topics[topicAndContentSpecCount].topicTitle.toString();
	row["ContentSpecs"] = topics[topicAndContentSpecCount].specs.toString();
	row["ContentSpecCount"] = topics[topicAndContentSpecCount].specCount;	
	
	++topicAndContentSpecCount;
	return true;
}

return false;]]></method>
