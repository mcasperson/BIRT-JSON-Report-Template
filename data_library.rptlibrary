            <method name="open"><![CDATA[logger.info("[BIRT Report Viewer] [open] [" + uuid + "] [Data Set] [SpecTopicAndTag]");

/*
	--------------------- Global variables ---------------------
*/

// specs mapped to topics
specTopics = [];

