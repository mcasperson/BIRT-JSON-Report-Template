// here we take the results of the queries against the spec ids
// and combine them into a single table

// use a function to limit variable scopes
(function()
{
	// find the specs that each topic is included in
	for (var index = 0; index < controllers.length; ++index)
	{		
		var controller = controllers[index];
		var topicCount = controller.getChildCount("items");
		
		for (var topicIndex = 0; topicIndex < topicCount; ++topicIndex) {
						
			var topicId = controller.getValue("items", "item/id", topicIndex);
			var topicTitle = controller.getValue("items", "item/title", topicIndex);

			var thisTopicDetails = null;

			if (topicIds.customIndexOf(topicId) == -1) {
				thisTopicDetails = {topicId: topicId, topicTitle: topicTitle, specCount: 1, specs: controllerCSPIDs[index].toString()};
				topicDetails[topicId] = thisTopicDetails;
				topicIds.push(topicId);
			} else {
				thisTopicDetails = topicDetails[topicId];
				thisTopicDetails.specCount += 1;
				thisTopicDetails.specs += "," + controllerCSPIDs[index];
			}
		}
	}
	
	for (var processedTopicIndex = 0; processedTopicIndex < topicIds.length; ++processedTopicIndex) {
		var thisTopicDetails = topicDetails[topicIds[processedTopicIndex]];
		topics.push(thisTopicDetails);
		
		if (thisTopicDetails.specCount == 1) {
			++singleReferences;
		} else {
			reusedReferences += thisTopicDetails.specCount;
		}
	}
})();]]></method>
